{
    "extends": ["airbnb-base","prettier"],
        "plugins": ["prettier"],
            "rules": {
                "no-console":"off",
                "no-underscore-dangle":"off",
                "func-names":"off",
        "prettier/prettier": ["error",{
            "endOfLine":"auto"
        }]
    },
    "parserOptions": {
        "ecmaVersion": 2020
    }
}